{
  "name": "ogp/uiapi",
  "description": "Reusable UI API package for Laravel 12",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Ogp\\UiApi\\Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Ogp\\UiApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ogp\\UiApi\\UiApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
