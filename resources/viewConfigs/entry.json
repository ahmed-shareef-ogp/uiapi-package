{
    "listView": {
        "components": {
            "searchBar": {
                "button": ["create", "edit"]
            },
            "table": {
                "button": ["close", "clear"],
                "actionButtons": ["edit", "delete", "view"]
            },
            "FilterSection": {
                "button": ["create", "edit"],
                "filters": ["gender", "country.name_eng"]
            }
        },
        "columns": [
            "id",
            "entry_type_id",
            "ref_num",
            "summary",
            "date_entry"
        ],
        "columnCustomizations": {
            "is_in_custody": {
                "title": {
                    "en": "ISINCUSTODY",
                    "dv": "މިވަގުތުބަންދުގައި"
                },
                "sortable": true,
                "hidden": false,
                "displayType": "checkbox",
                "inlineEditable": true,
                "displayProps": {
                    "color": "primary"
                }
            },
            "gender": {
                "sortable": true,
                "hidden": false,
                "displayType": "select",
                "inlineEditable": true,
                "displayProps": {
                    "items": [
                        { "title":"Male", "value":"M" },
                        { "title": "Female", "value": "F" }
                    ]
                }
            }
        },
        "filters": ["id"],
        "per_page": 10,
        "lang": ["en", "dv"]
    }
}
